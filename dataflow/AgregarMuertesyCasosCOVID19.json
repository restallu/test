{
	"name": "AgregarMuertesyCasosCOVID19",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CSVFileWeb_Case_Deaths",
						"type": "DatasetReference"
					},
					"name": "source1",
					"description": "Import data from CSVFileWeb_Case_Deaths"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "pivot1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          country_code as string,",
				"          continent as string,",
				"          population as string,",
				"          indicator as string,",
				"          daily_count as string,",
				"          date as string,",
				"          rate_14_day as string,",
				"          {source\\r} as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     maxColumns: 50) ~> source1",
				"source1 pivot(pivotBy(indicator),",
				"     columnNaming: '$N$V',",
				"     lateral: true) ~> pivot1"
			]
		}
	}
}