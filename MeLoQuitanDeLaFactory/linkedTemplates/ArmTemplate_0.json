		{
			"name": "[concat(parameters('factoryName'), '/AgregarMuertesyCasosCOVID19')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "CSVFileBLOB_Case_Deaths",
								"type": "DatasetReference"
							},
							"name": "ExtractCasesDeaths",
							"description": "Import data from CSVFileWeb_Case_Deaths"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "CSVFileBLOB_Case_Deaths_Out",
								"type": "DatasetReference"
							},
							"name": "LoadToDataLake",
							"rejectedDataLinkedService": {
								"referenceName": "AzureBlobStorage1",
								"type": "LinkedServiceReference"
							}
						}
					],
					"transformations": [
						{
							"name": "pivot1"
						},
						{
							"name": "FiltrarContinenteEuropa"
						},
						{
							"name": "MapDrifted1",
							"description": "Creates an explicit mapping for each drifted column"
						}
					],
					"scriptLines": [
						"source(output(",
						"          country as string,",
						"          country_code as string,",
						"          continent as string,",
						"          population as integer,",
						"          indicator as string,",
						"          daily_count as integer,",
						"          date as date 'yyyy-MM-dd',",
						"          rate_14_day as float,",
						"          source as string",
						"     ),",
						"     allowSchemaDrift: false,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false,",
						"     maxColumns: 50) ~> ExtractCasesDeaths",
						"FiltrarContinenteEuropa pivot(groupBy(country,",
						"          country_code,",
						"          continent,",
						"          date,",
						"          population),",
						"     pivotBy(indicator),",
						"     {} = sum(daily_count),",
						"     columnNaming: 'Total_$N$V',",
						"     lateral: true) ~> pivot1",
						"ExtractCasesDeaths filter(continent==\"Europe\") ~> FiltrarContinenteEuropa",
						"pivot1 derive({Total_confirmed cases} = toLong(byName('Total_confirmed cases')),",
						"          Total_deaths = toLong(byName('Total_deaths'))) ~> MapDrifted1",
						"MapDrifted1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          Column_1 as string,",
						"          Column_2 as string,",
						"          Column_3 as string,",
						"          Column_4 as string,",
						"          Column_5 as string,",
						"          Column_6 as string,",
						"          Column_7 as string",
						"     ),",
